extends ../../_includes/_layout.pug

append variables
  //- Required
  - var pageID = "posse";
  - var pageTitle = "ANTI-PATTERN HUB - POSSE";
  - var pageDescription = site.description;
  //- Optional
  - var pageOgpImage = site.ogpImage
  - var pageOgpType = "article";
  
block styles
  link(rel="stylesheet" type="text/css" href="../../css/pages/hub.min.css")


block content
