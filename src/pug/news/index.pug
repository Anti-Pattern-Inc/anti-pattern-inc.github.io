extends ../_includes/_layout.pug

append variables
  //- Required
  - var pageID = "news";
  - var pageTitle = "ニュース";
  - var pageDescription = site.description;
  //- Optional
  - var pageOgpImage = site.ogpImage
  - var pageOgpType = "article";

block styles
  link(rel="stylesheet" type="text/css" href="../../css/pages/news.min.css")



block content
  section.keyvisual
    p
      span.key_title_main NEWS
      |
      span.key_title_sub お知らせ
  .news_container
    .news_contents
      a(href="/news/announcement-of-seed-round-financing/")
        .a_news
          time(datetime="2022-04-05") 2022.04.05
          br.sp_br
          p シードラウンドにて9,200万円の資金調達を実施しました。
      a(href="/news/day-one-cto-vpoe-conference-2022-spring/")
        .a_news
          time(datetime="2022-03-23") 2022.03.23
          br.sp_br
          p 日本CTO協会が主催の「Day One - CTO/VPoE Conference 2022 Spring - 」に代表 小笹が登壇
      a(href="/news/engineed-accepting-applications-for-exam-of-trouble-shooting-vol1/")
        .a_news
          time(datetime="2022-02-09") 2022.02.09
          br.sp_br
          p engineed、新たな実技試験「トラブルシューティング問題 Vol.1」を公開！
      a(href="/news/engineed-posted-on-aws-japan-apn-blog/")
        .a_news
          time(datetime="2022-01-18") 2022.01.18
          br.sp_br
          p engineedがAWS JAPAN APNブログに掲載されました
      a(href="/news/regional-scrum-gathering-tokyo-2022/")
        .a_news
          time(datetime="2022-01-05") 2022.01.05
          br.sp_br
          p Regional Scrum Gathering℠ Tokyo 2022 に協賛いたしました。
      a(href="/news/engineed-accepting-applications-for-exam-of-wordpress")
        .a_news
          time(datetime="2021-11-24") 2021.11.24
          br.sp_br
          p engineed、AWSエンジニア向けのWordPressを使用した新規実技試験を公開！
      a(href="/news/certified-as-aws-isv-partner/")
        .a_news
          time(datetime="2021-11-11") 2021.11.11
          br.sp_br
          p engineedがAWSファンデーショナルテクニカルレビューを通過しました
      a(href="/news/release-saasus-consulting/")
        .a_news
          time(datetime="2021-10-01") 2021.10.01
          br.sp_br
          p ソフトウェア開発内製化を支援しDXを推進する新サービス「SaaSus Consulting」をリリース
      a(href="/news/certified-as-aws-consulting-partner-select-tier/")
        .a_news
          time(datetime="2021-07-27") 2021.07.27
          br.sp_br
          p AWSコンサルティングパートナー・セレクトティアに認定されました
      a(href="/news/engineed-accepting-applications-for-exam-of-ecs/")
        .a_news
          time(datetime="2021-07-08") 2021.07.08
          br.sp_br
          p engineed、AWSエンジニア向けのECSをベースにした新規実技試験を公開！
      a(href="/news/yagasaki-was-appointed-as-a-director/")
        .a_news
          time(datetime="2021-06-29") 2021.06.29
          br.sp_br
          p 当社の取締役CTO兼COOに矢ヶ崎哲宏が就任
      a(href="/news/engineed-accepting-applications-for-exam-of-ec2/")
        .a_news
          time(datetime="2021-03-01") 2021.03.01
          br.sp_br
          p AWS有資格者限定マッチングプラットフォームengineedが実技試験の申し込みを開始しました
      a(href="/news/release-engineed-public-beta-ver/")
        .a_news
          time(datetime="2021-02-10") 2021.02.10
          br.sp_br
          p AWSエンジニアを採用できるサービスengineedのPublic Beta版をリリースしました
      a(href="/news/release-posse/")
        .a_news
          time(datetime="2020-07-28") 2020.07.28
          br.sp_br
          p 大学生限定のプログラミング学習コミュニティ「POSSE」をスタート！
      a(href="/news/release-engineed-preview-ver/")
        .a_news
          time(datetime="2020-06-04") 2020.06.04
          br.sp_br
          p AWSエンジニア向け実技試験型案件獲得サービスengineedのプレビュー版事前登録開始
      a(href="/news/30daysbootcamp-on-harbors/")
        .a_news
          time(datetime="2020-03-31") 2020.03.31
          br.sp_br
          p エンジニアコミュニティ「HarborS」で、東京フリーランスが「30DAYSブートキャンプ」を開催！
      a(href="/news/harbors-open/")
        .a_news
          time(datetime="2020-02-29") 2020.02.29
          br.sp_br
          p エンジニア特化型コワーキング事業「HarborS」をリリース。第一弾として表参道拠点をオープン！

